import com.nt.constant.PassportAppConstant;

@Controller
public class SuccessPageController {
		@RequestMapping("/sussPage")
		public String showSuccPage(HttpServletRequest req) {
			return PassportAppConstant.SUCC_PAGE_STR;
		}
}
